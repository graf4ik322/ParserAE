# Telegram Bot Configuration
BOT_TOKEN=YOUR_BOT_TOKEN_HERE
ADMIN_USER_ID=0

# OpenRouter API Configuration
OPENROUTER_API_KEY=YOUR_OPENROUTER_API_KEY_HERE
OPENROUTER_MODEL=anthropic/claude-3-haiku
OPENROUTER_MAX_TOKENS=1000
OPENROUTER_TEMPERATURE=0.7

# Bot Settings
MAX_MESSAGE_LENGTH=4096
QUIZ_TIMEOUT=300
CACHE_SIZE=100

# LLM Limits
QUESTION_LIST_LIMIT=200
QUIZ_LIST_LIMIT=300
FACTORY_SUMMARY_LIMIT=10
TOP_FACTORIES_LIMIT=8

# Text Formatting Settings
MAX_EMOJIS_PER_MESSAGE=4
MAX_LINES_PER_BLOCK=6

# Auto Parser Settings
AUTO_PARSER_ENABLED=true
PARSE_SCHEDULE_HOURS=6
DAILY_PARSE_TIME=06:00
WEEKLY_PARSE_DAY=sunday
WEEKLY_PARSE_TIME=03:00
PARSER_MAX_WORKERS=2
SEND_NOTIFICATIONS=true